{
  "name": "belt-repo",
  "version": "0.0.5",
  "description": "repo module for belt",
  "main": "cli.js",
  "author": "ewnd9 <ewndnine@gmail.com>",
  "license": "MIT",
  "repository": "https://github.com/ewnd9/belt/tree/master/packages/belt-repo",
  "belt": {
    "commands": {
      "repo:belt-command": {
        "script": "commands/repo-belt-command",
        "description": "create new command for cwd package.json"
      },
      "repo:open": {
        "script": "commands/repo-open",
        "description": "open repo in a browser"
      },
      "repo:open-commit": {
        "script": "commands/repo-open-commit",
        "description": "open current HEAD in a browser"
      }
    }
  },
  "dependencies": {
    "belt-tools": "^0.0.5",
    "git-url-parse": "^8.2.0"
  }
}
